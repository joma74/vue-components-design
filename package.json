{
  "name": "vue-components-design",
  "version": "0.0.1",
  "description": "after adam wathans component design course",
  "main": "index.js",
  "author": "joma74 <dev-mails@gmx.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "tsc_validate_watch": "yarn tsc_validate_once -- --watch",
    "tsc_validate_once": "node_modules/typescript/bin/tsc --project ./tsconfig.json --noEmit true --traceResolution true",
    "_lint-js": "tslint --project '.' 'src/**/*.ts' 'src/**/*.tsx' './config/**/*.js'",
    "lint-js-fix": "yarn run _lint --fix",
    "check-staged": "lint-staged"
  },
  "resolutions": {
    "**/typescript": "~3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc_validate_once && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn run _lint --"
    ],
    "*.{js,ts,tsx,json,md}": [
      "prettier --list-different"
    ]
  },
  "devDependencies": {
    "@types/node": "^10.11.5",
    "autoprefixer": "^9",
    "husky": "^1",
    "lint-staged": "^7",
    "prettier": "^1",
    "tailwindcss": "^0",
    "tslint": "^5",
    "tslint-config-prettier": "^1",
    "tslint-eslint-rules": "^5",
    "typescript": "^3"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8.12.0",
    "yarn": ">= 1.7.0"
  }
}
